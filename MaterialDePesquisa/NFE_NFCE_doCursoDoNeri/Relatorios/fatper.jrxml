		<![CDATA[select
	cod_cliente,
	valor_total,
	numero_nfe,
          serie_nfe,
	modelonfe,
	data_emissao,
          doc_cliente,
	cliente.razaosocial as razao
from
     nf,
     cliente
where
      nf.tpamb = $P{tipoAmbiente} and
      cod_cliente = cliente.codigo and
      data_emissao >= $P{data_ini} and
      data_emissao <= $P{data_fim} and
      numero_nfe > 0
order by
      data_emissao, modelonfe, numero_nfe]]>
