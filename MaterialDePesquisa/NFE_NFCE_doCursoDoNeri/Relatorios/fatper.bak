	<queryString><![CDATA[select 
        NF.cod_cliente, 
	sum(NF.valor_total) as vlrtot,
	sum(NF.valor_descontos) as vlrdesc,
	sum(NF.valor_produtos) as vlrnf,
	cliente.razaosocial as razao,
	(select sum(NFX.valor_produtos) from nf as NFX, cliente  
	where
	      	NFX.cod_cliente = cliente.codigo and
	      NFX.data_emissao >= $P{data_ini} and 
	      NFX.data_emissao <= $P{data_fim} 

) as somavlr 

from 
     nf AS NF,
     cliente  
where 
      NF.cod_cliente = cliente.codigo and
      data_emissao >= $P{data_ini} and 
      data_emissao <= $P{data_fim}
group by 
      NF.cod_cliente, cliente.razaosocial
order by
      sum(NF.valor_total) desc]]></queryString>

